  "version": "1.0.0",
  "main": "net.thenova.titan.spigot.module.vouchers.Vouchers",
  "permissions": {
    "titan.voucher.admin": {
      "default": "OP",
      "description": "Use all the voucher commands",
      "children": {
        "titan.command.voucher.use": true,
        "titan.command.voucher.give": true,
        "titan.command.voucher.list": true,
        "titan.command.voucher.reload": true
      }
    },
    "titan.command.voucher.use": {
      "default": "OP",
